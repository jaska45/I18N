<Window 
  x:Class="Tutorial.MainWindow"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  Title="Sample" Height="220" Width="500" Loaded="Window_Loaded">
  
  <Grid Margin="8">
    <Grid.RowDefinitions>
      <RowDefinition Height="auto" />
      <RowDefinition />
      <RowDefinition />
      <RowDefinition />
      <RowDefinition />
      <RowDefinition />
    </Grid.RowDefinitions>      
    
    <StackPanel Orientation="Horizontal" Height="30" Margin="0,0,0,8">
      <Label>_Filename:</Label>
      <TextBox Name="textBox1" Width="325" Height="24" Margin="8,0,8,0" />
      <Button Width="60" Click="Button_Click">_Browse...</Button>
    </StackPanel>
    
    <CheckBox Grid.Row="1" VerticalAlignment="Center" Width="110" HorizontalAlignment="Left" Content="Compress data" />
    
    <Label Name="label2" Grid.Row="2" Width="80" HorizontalAlignment="Left">label2</Label>
    <Label Name="label3" Grid.Row="3">label3</Label>
    <Label Name="label4" Grid.Row="4">label4</Label>
    <Label Name="label5" Grid.Row="5">label5</Label>
  </Grid>
</Window>
